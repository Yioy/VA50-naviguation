<launch>

    <arg name="config_file_arg" default="$(find bringup)/config/circulation4-2.yml"/>
    
    <param name="config_file" value="$(arg config_file_arg)" />

    <env name="ROSCONSOLE_FORMAT" value="[${severity}] [${node}]: ${message}"/>
  
    <!-- Node for circulation -->
    <node pkg="circulation" type="circulation4.py" name="circulation4_node" output="screen"/>
  
</launch>